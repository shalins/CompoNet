{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "componet": "link:./componet",
    "long": "^5.2.3",
    "plotly.js": "^2.18.0",
    "postgres": "^3.4.3",
    "protobufjs": "^7.2.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "serve": "^14.2.1",
    "yarn": "v2.4.3"
  },
  "devDependencies": {
    "@types/plotly.js": "^2.12.5",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/react-plotly.js": "^2.5.2",
    "eslint": "^8.0.0",
    "eslint-config-react-app": "^7.0.1",
    "ts-proto": "^1.167.2",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "proto": "mkdir -p ./src/proto/ts/ && protoc --ts_proto_opt=esModuleInterop=true --plugin=./node_modules/.bin/protoc-gen-ts_proto --proto_path=./src/proto/ --ts_proto_out=./src/proto/ts/ ./src/proto/*.proto",
    "build-rs": "cargo build --release && wasm-bindgen target/wasm32-unknown-unknown/release/componet.wasm --target=web --out-dir=componet --omit-default-module-path && cp componet/componet_bg.wasm public/",
    "debug": "yarn proto && yarn build-rs && react-scripts start",
    "start": "serve -s build",
    "build": "yarn react-scripts build",
    "test": "yarn react-scripts test",
    "eject": "yarn react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "proxy": "http://localhost:5555",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}